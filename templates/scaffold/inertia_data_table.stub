import { createApp, h } from "vue";
import { createInertiaApp, Link } from "@inertiajs/inertia-vue3";

createInertiaApp({
    id: "dataTable",
    resolve: (name) => require(`./dataTables/${name}`),
    setup({ el, App, props, plugin }) {
        createApp({ render: () => h(App, props) })
            .use(plugin)
            .component("InertiaLink", Link)
            .mount(el);
    },
});
