{
  "name": "infyomlabs/laravel-generator",
  "type": "library",
  "description": "InfyOm Laravel Generator",
  "keywords": [
    "laravel",
    "api",
    "model",
    "request",
    "migration",
    "model",
    "crud",
    "repository",
    "view",
    "test",
    "generator",
    "swagger"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Mitul Golakiya",
      "email": "me@mitul.me"
    }
  ],
  "require": {
    "php": "^7.4|^8.0",
    "illuminate/support": "^8.0|^9.0|^10.0|^11.0|^12.0",
    "laracasts/flash": "^3.2"
  },
  "require-dev": {
    "phpunit/phpunit": "^10.0|^11.0",
    "mockery/mockery": "^1.3.1",
    "driftingly/rector-laravel": "^2.0"
  },
  "scripts": {
    "code-lint": "php-cs-fixer fix --diff --dry-run",
    "code-lint-fix": "php-cs-fixer fix",
    "rector": "vendor/bin/rector process"
  },
  "conflict": {
    "phpunit/phpunit": ">=12.0"
  },
  "autoload": {
    "psr-4": {
      "InfyOm\\Generator\\": "src/"
    },
    "files": [
      "src/helpers.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "\\InfyOm\\Generator\\InfyOmGeneratorServiceProvider"
      ]
    }
  },
  "funding": [
    {
      "type": "opencollective",
      "url": "https://opencollective.com/infyomlabs"
    }
  ]
}
